<odoo>
    <template id="report_customer_card_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Customer Card - <span t-field="doc.name"/></h2>
                        <ul>
                            <li>Gender: <span t-field="doc.gender"/></li>
                            <li>Country: <span t-field="doc.country_id.name"/></li>
                            <li>Joining Date: <span t-field="doc.joining_date"/></li>
                        </ul>
                        <p>Last Updated: <span t-field="doc.last_update_date"/></p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>